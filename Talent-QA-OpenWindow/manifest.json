{
  "manifest_version": 2,
  "name": "Talent QA Editor",
  "description": "Web Test Recorder",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "assets/icons/icon_TestLab01.png",
    "default_title": "RCR Example",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "assets/images/icon_TestLab01.png"
   }
}
